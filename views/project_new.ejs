<% include header.ejs %>
<div class="jumbotron">
<form role="form">
  <div class="form-group">
    <label for="exampleInputEmail1">プロジェクト名</label>
    <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email">
  </div>
  <div class="form-group">
    <label for="description">説明</label>
    <textarea class="form-control" name="description" rows="10"></textarea>
    <p class="help-block">ライセンスなどもこちらに入力して下さい。</p>
  </div>
  <div class="form-group">
    <label for="addcol">入力項目</label>
    <button name="addcol" type="button" class="btn">追加</button>
  </div>
  <div class="row">
    <div class="col-xs-3">
      <input name="col[0]" class="form-control">
      <button name="delcol[0]" class="btn">削除</button>
    </div>
  </div>
  <button type="submit" class="btn btn-default">送信</button>
</form>
</div>
